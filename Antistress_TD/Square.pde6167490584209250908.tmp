class Square {
  int x1, y1, x2, y2;
  Button button;
  Tower tower;
  
  Square(int x1_, int y1_, int x2_, int y2_) {
    x1 = x1_;
    y1 = y1_;
    x2 = x2_;
    y2 = y2_;
    button = new Button(x1, y1, x2, y2);
  }
  
  void display() {
    if (draggingStatus > 0 && button.collision()) fill(220);
    else fill(255);
    rect(x1, y1, x2, y2);
  }
  
  
}
